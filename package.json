{
  "name": "nissan_service_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev": "ENVFILE=.env react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:prod": "ENVFILE=.env.prod react-native run-android",
    "build-android:dev": "cd ./android && ./gradlew clean && ./gradlew assembleRelease",
    "build-android:staging": "cd ./android && ./gradlew clean && cross-env ENVFILE=.env.staging ./gradlew assembleRelease",
    "build-android:production": "cd ./android && ./gradlew clean && cross-env ENVFILE=.env.prod ./gradlew assembleRelease",
    "generate-dotenv": "sh util/generate-dotenv.sh > .env",
    "device:adb": "adb reverse tcp:8081 tcp:8081",
    "clean": "yarn cache clean && cd android && ./gradlew clean && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd .."
  },
