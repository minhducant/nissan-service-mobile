{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@configs/*": ["src/configs/*"],
      "@context/*": ["src/context/*"],
      "@i18n/*": ["src/i18n/*"],
      "@pages/*": ["src/pages/*"],
      "@stores/*": ["src/stores/*"],
      "@repository/*": ["src/repository/*"],
      "@routes/*": ["src/routes/*"],
      "@styles/*": ["src/styles/*"],
      "@common/*": ["src/common/*"],
      "@services/*": ["src/services/*"],
      "@data/*": ["src/data/*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
